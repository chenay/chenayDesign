<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <!-- 描边 -->
            <stroke android:width="@dimen/dp_1" android:color="@color/color_chy_stroke_1" android:dashWidth="0dp" android:dashGap="0dp" />
        </shape>

    </item>
    <item android:state_selected="true">
        <shape>
            <!-- 描边 -->
            <stroke android:width="@dimen/dp_1" android:color="@color/color_chy_stroke_1" android:dashWidth="0dp" android:dashGap="0dp" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <!-- 描边 -->
            <stroke android:width="@dimen/dp_1" android:color="@color/color_chy_stroke_1" android:dashWidth="0dp" android:dashGap="0dp" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <!-- 描边 -->
            <stroke android:width="@dimen/dp_1" android:color="@color/color_chy_stroke" android:dashWidth="0dp" android:dashGap="0dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <!-- 描边 -->
            <stroke android:width="@dimen/dp_1" android:color="@color/color_chy_stroke" android:dashWidth="0dp" android:dashGap="0dp" />
        </shape>
    </item>
</selector>